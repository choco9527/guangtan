.search-page .c-search {
  background: transparent !important;
  max-width: calc(100vw - 120px);
  padding-left: 0;
}
.search-page .main-list {
  max-height: calc(100vh - 66px);
  box-sizing: border-box;
  padding: 8px;
}
.search-page .main-list .list-item {
  display: flex;
  justify-content: space-between;
  align-items: stretch;
  margin-bottom: 8px;
  background-color: #fff;
  padding: 8px;
  border-radius: 4px;
}
.search-page .main-list .list-item .left {
  margin-right: 8px;
}
.search-page .main-list .list-item .right {
  font-size: 12px;
  flex: 1;
}
.search-page .main-list .list-item .right .created {
  color: #666;
}

